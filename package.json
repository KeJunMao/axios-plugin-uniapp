{
  "name": "@uni-helper/axios-adapter",
  "type": "module",
  "version": "0.0.2",
  "license": "MIT",
  "packageManager": "pnpm@7.12.0",
  "description": "The Axios adapter for uniapp",
  "homepage": "https://github.com/KeJunMao/axios-plugin-uniapp",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KeJunMao/axios-plugin-uniapp.git"
  },
  "keywords": [
    "uniapp",
    "axios",
    "uni-app",
    "axiosAdapter"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "start": "esno src/index.ts",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@dcloudio/types": "^3.0.19",
    "@types/node": "^18.11.9",
    "bumpp": "^8.2.1",
    "eslint": "^8.27.0",
    "esno": "^0.16.3",
    "pnpm": "^7.15.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4",
    "unbuild": "^0.8.11",
    "vite": "^3.2.3"
  },
  "dependencies": {
    "axios": "^0.27.2"
  }
}
